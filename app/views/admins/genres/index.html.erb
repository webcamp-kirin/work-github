<h1>ジャンル一覧・追加</h1>

<%= form_with model:@genre,url: admins_genres_path, method: :post, local:true do |f| %>
<table>
  <thead>
    <tr>
  　 <th>ジャンル名</th>
  　  <th></th>
     <th><%= f.text_field :name %></th>
     <th></th>
     <th><%= f.submit '新規登録' %></th>
    </tr>
  </thead>
</table> 

<% @genres.each do |genre| %>
<table class='table'>
 <tr>
  <th>
  <%= genre.name %>
  </th>
  <th></th>
  <th>
  <%=link_to  edit_admins_genre_path(genre) do %>
  <%= f.submit '編集する' %>
  <% end %>
  </th>
 </tr>
</div>
<% end %>
<% end %>